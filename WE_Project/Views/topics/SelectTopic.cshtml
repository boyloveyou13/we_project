@model IEnumerable<WE_Project.Models.topic>

@{
    ViewBag.Title = "SelectTopic";
}

@foreach (var item in Model)
{
    if (DateTime.Compare((DateTime)item.final_date, DateTime.Now.Date) >= 0)
    {
        <a class="collapse-item" href="@Url.Action("Index", "ideas",  new { id= item.topic_id, sort=1 })">@Html.DisplayFor(modelItem => item.topic_name)</a>
    }

}